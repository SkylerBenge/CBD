}

.site-header__search {
  display: inline-block;
  max-width: 400px;
  margin-top: $gutter / 2;
  width: 100%;
}

.search-bar {
  max-width: 100%;
  @include at-query ($max, $medium) {
    margin-left: auto;
    margin-right: auto;
  }
}

/*============================================================================
  #Site Nav and Dropdowns
==============================================================================*/
.nav-bar {
  background-color: $colorTextBody;
}
.site-nav {
  @include size-body(16px);
  cursor: default;
  margin: 0 0 0 (-$gutter / 2);
  list-style: none;
}
.site-nav__item {
  position: relative;
  display: inline-block;
  margin: 0;

  li {
    display: block;
  }
  .custom-icon-hamburger, .custom-icon-down-arrow {color:$navColor !important;}
}

.custom-icon-cart {
  display: inline-block;
  font-size: 32px;
  color: $navColor !important;
  vertical-align: middle;
}

#CartCount {
    bottom: 3px;
    position: absolute;
    right: -7px;
    display: block;
    text-align: center;
    font-style: normal;
    color: opposite($colorAccent);
    font-weight: bold;
    text-transform: none;
    text-transform: none;
    background: $colorAccent;
    border-radius: 50%;
    width: 24px;
    height: 24px;
    font-size:12px;
    line-height:24px;
    display: inline-block;
    vertical-align: top;
}

.drawer-nav__toggle {
  padding:10px;
}

.drawer-nav__toggle {
  .custom-icon-down-arrow {
    color: $colorTextBody;
  }
  .custom-icon-down-arrow:before{
    vertical-align: bottom;
  }
}

/*================ Home to hamburger and cart that toggle drawers ================*/
.site-nav--mobile {
  @extend .site-nav;

  .text-right & {
    margin: 0 (-$gutter / 2) 0 0;
  }
}
.site-nav__link:active {
  text-decoration: underline;
}

.site-nav__link {
  @include size-body(16px);
  display: inline-block;
  text-decoration: none;
  padding:15px 15px;
  white-space: nowrap;
  color: $navColor;
  font-family: $body_font;
  font-weight:$body_weight;

  .site-nav--mobile & {
    display: inline-block;
  }


  &:hover,
  &:active,
  &:focus {
    color: darken($navColor, 10%); 
  }

}

.custom-icon-hamburger{
  font-size: 22px;
  margin-bottom: -8px;
  float:right;
  color: $colorTextHeading;
  @include at-query ($min, 991px) {
    color: $colorTextBody;
  }
}

.site-header__logo a {display:inline-block; color: $navColor !important;}
.site-header h1 {line-height:100%}
.site-header__logo {line-height:100%;}
.header-offset{display:none;}


/*================ Dropdowns ================*/
.site-nav__dropdown {
  display: none;
  background: #fff;
  position: absolute;
  left: 0;
  margin: 0;
  margin-left:25px;
  z-index: $zindexNavDropdowns;

  .supports-no-touch .site-nav--has-dropdown:hover &,
  .site-nav--has-dropdown.nav-hover &,
  .nav-focus + & {
    display: block;
  }

  a {
    color: $colorTextBody;

    &:hover,
    &:active,
    &:focus {
       color: darken($colorTextBody, 10%); 
    }
  }
}
.full-header .site-nav__dropdown a { color: $colorTextBody;  }
.cssmenu ul ul li:last-child {
  border-bottom: none;
}

.site-nav__dropdown a {padding: 8px 15px;}
.site-nav__dropdown {
  //border: 1px solid $colorBorder;
  text-align:left;
}

.site-nav--compress {
  .site-nav__expanded-item {
    display: none;
  }
}

.site-nav--compress__menu {
  display: none;

  .site-nav--compress & {
    display: inline-block;
  }
}

.site-header__cart-toggle {
  margin: 0 0 0 10px;
}

.drawer-nav__has-sublist {
  display:inline-block;
  margin-right: -20px;
}

/*============================================================================
  #Mobile Nav
  - List of items inside the mobile drawer
==============================================================================*/
.mobile-nav {
  // Negative of .drawer left/right padding for full-width link tap area
  margin: 0 (-$gutter / 2) 0 (-$gutter / 2);
}

.mobile-nav__search {
  padding: $gutter / 2;

  .search-bar {
    margin-bottom: 0;
  }
}

.mobile-nav__item {
  position: relative;
  display: block;

  // Background color on top level items so there is no
  // element overlap on subnav's CSS toggle animation
  .mobile-nav > & {
    background-color: $colorBody;
  }

  .mobile-nav > &:last-child:after {
    display: none;
  }
}

// Login/logout links can't have a class on them, so style <a> element
.mobile-nav__item a {
  display: block;
}

.mobile-nav__item a,
.mobile-nav__toggle button {
  color: $colorTextHeading;
  text-decoration: none;

  &:active,
  &:focus {
   background-color: setActiveDrawers($colorBody);
  }
}

.mobile-nav__item--active {
  font-weight: inherit;
}

.mobile-nav__has-sublist {
  display: table;
  width: 100%;

  .mobile-nav__link {
    margin-top: 0;
    vertical-align: middle;
    width: 100%;
  }
}

.mobile-nav__link {
  line-height:1;
  margin-top:10px;  
  padding-top: 10px;
  padding-bottom: 10px;
  padding-left: 20px;
  font-size: 18px;

}
.mobile-nav__link.child-link {
  font-size: 15px;
  font-weight: inherit !important;
}

.mobile-nav__toggle {
  display: table-cell;
  vertical-align: middle;
  width: 50%;
  padding-left: 50px;
  color: $colorTextHeading;
}

.mobile-nav__toggle-open {
  width: 100%;
  .mobile-nav--expanded & {
    display: none;
  }
}

.mobile-nav__toggle-close {
  width: 100%;
  display: none;

  .mobile-nav--expanded & {
    display: block;
  }
}

.mobile-nav__sublist {
  margin: 0;
  max-height: 0;
  visibility: hidden;
  overflow: hidden;
  transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  margin-left: 20px;
  @include backface();

  .mobile-nav--expanded + & {
    visibility: visible;
    max-height: 700px;
    transition: all 700ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  }

  .mobile-nav__item:after {
    top: 0;
    bottom: auto;
  }

  .mobile-nav__link {
    font-weight: normal;
  }

  :last-child {
    padding-bottom:0px;
  }
}

.show-dropdown {
  visibility: visible;
  max-height: inherit;
}

.js-drawer-close {
  .custom-icon-x{
    color: $colorTextHeading;
  }
}

/*============================================================================
  #Drawers
==============================================================================*/
.js-drawer-open {
  height: 100%;
}

.drawer {
  @include promote-layer();
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
  top: 0;
  bottom: 0;
  padding: 0 ($gutter / 2) ($gutter / 2);
  max-width: 95%;
  z-index: $zindexDrawer;
  color: $colorDrawerText;
  background-color: $colorBody;
  transition: $drawerTransition;

  a, h3 {
    color: $colorTextHeading;
    &:hover,
    &:focus,
    &:active {
      transition: all 0.2s ease;
      color: darken($colorTextHeading,20%);
    }
  }

  input {
    border: none;
  }
  textarea {
    border-color: $colorBorder;
    border-width:1px;
  }
  .shipping-message{
    text-align:center;
    padding:10px;
    color:#fff;
    background:$colorBtnPrimary;
    margin:8px 0;
    font-weight:400;
  }
}

.drawer--left {
  width: $drawerNavWidth;
  right: -$drawerNavWidth;
  border-right: 1px solid $colorDrawerBorder;
  border-left: 2px solid $colorAccent;

  .js-drawer-open-left & {
    display: block;
    @include transform(translateX(-$drawerNavWidth));

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  width: $drawerCartWidth;
  right: -$drawerCartWidth;
  
  border-left: 2px solid $colorAccent;

  .js-drawer-open-right & {
    display: block;
    @include transform(translateX(-$drawerCartWidth));

    .lt-ie9 & {
      right: 0;
    }
  }
}

#PageContainer {
  overflow: hidden;
  background: inherit;
}
.full-header #PageContainer {
    margin-top: -90px;
}
.is-moved-by-drawer {
  @include promote-layer();
  transition: $drawerTransition;
 will-change: initial;
  .js-drawer-open-left & {
    @include transform(translateX(-$drawerNavWidth));
  }

  .js-drawer-open-right & {
    @include transform(translateX(-$drawerCartWidth));
  }
}

.drawer__header {
  display: table;
  height: 50px;
  width: 100%;
}

.drawer__title,
.drawer__close {
  display: table-cell;
  vertical-align: middle;
}

.drawer__title {
  width: 100%;
}

.drawer__close {
  width: 1%;
  text-align: center;
  font-size: em(18px);
}

.drawer__close button {
  position: relative;
  right: -20px;
  height: 100%;
  padding: 0 20px;
  color: inherit;

  &:active,
  &:focus {
    .custom-icon-x {
      color: lighten($colorTextHeading, 20%) !important;
    }
  }
  .custom-icon-x{
    font-size:20px;
    vertical-align: bottom;
  }
}

.cart-drawer-spacer-line{
  border-bottom: 2px solid $colorAccent;
  clear: both;
  display: block;
  width: 100%;
  margin-bottom: 10px;
}
.drawer-button{margin-top:20px;}

#NavDrawer button {
  float: right;
  z-index: 1000;
  height: 38px;
}
/*============================================================================
  #Product and Collection Grids
==============================================================================*/
.grid__image {
  position: relative;
  display: block;
  margin: 0 auto ($gutter / 2);

  img {
    display: block;
    margin: 0 auto;
  }
  &.on-bottom{margin-top:30px;}
}

/*============================================================================
  #Collection Filters
==============================================================================*/
.filter--active {
  font-weight: bold;
}

/*============================================================================
  #Breadcrumbs
==============================================================================*/
.breadcrumb {
  padding-left: 50px;
  margin-bottom: 0;
  border-radius: 0;
  a,
  span {
    display: inline-block;
    padding: 0 7px 0 0;
    margin-right: 7px;

    &:first-child {
      padding-left: 0;
    }
  }
}

/*============================================================================
  #Notes and Form Feedback
==============================================================================*/
.note,
.errors {
  border-radius: $radius;
  padding: 6px 12px;
  margin-bottom: $gutter / 2;
  border: 1px solid transparent;
  @include size-body(12px);
  text-align: center;

  ul,
  ol {
    margin-top: 0;
    margin-bottom: 0;
    list-style: none outside;
  }

  li:last-child {
    margin-bottom: 0;
  }

  p {
    margin-bottom: 0;
  }
}

.errors {
  background-color:white;
  ul {
    list-style: none outside;
    margin-left: 20px;
  }
}

.form-success {
  color: #fff;
  border-color: #e7a61a;

  a {
    color: $successGreen;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}

.form-error,
.errors {
  color: $errorRed;
  border-color: $errorRed;

  a {
    color: $errorRed;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}


/*============================================================================
  #Ajax Cart Styles (conditionally loaded)
==============================================================================*/
.ajaxcart__inner {
  margin-bottom: 20px;
}

.ajaxcart__row {
  > .grid {
    margin-left: -$gutter / 2;

    > .grid__item {
      padding-left: $gutter / 2;
    }
  }
}

.ajaxcart__product {
  position: relative;
  max-height: 500px;

  &.is-removed {
    max-height: 0;
    overflow: hidden;
    visibility: hidden;
    transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
    @include backface();
  }
}

.ajaxcart__row {
  padding-bottom: 5px;
  margin-bottom: 5px;
  p {
    margin-bottom: 0px;
  }
}

.ajaxcart__product-image {
  display: block;
  overflow: hidden;
  margin-bottom: 15px;
  margin-top:20px;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }
}

.ajaxcart__product-name {
  display: block;
  color: $colorTextHeading !important;
  @include size-header(16px);
  &:hover,
  &:focus,
  &:active {
  color: $colorLinkHover !important;
  }
}


.ajaxcart__product-meta {
  color: $colorTextLight;
}

.ajaxcart_product-price {display:block;}

#CartContainer {color: $colorTextBody;}

#CartDrawer .custom-icon-x {color: $colorTextHeading !important;}

/*============================================================================
  #Quantity Selectors
==============================================================================*/
.js-qty {
  position: relative;
  margin-bottom: 1em;
  max-width: 100px;
  min-width: 75px;
  overflow: visible;

  input {
    display: block;
    background: none;
    text-align: center;
    width: 100%;
    padding: 5px 25px;
    border-width:2px;
    font-weight: 700;
    margin: 0;
    color: $colorTextBody;
  }
}

.js-qty__adjust {
  cursor: pointer;
  position: absolute;
  display: block;
  top: 0;
  bottom: 0;
  border: 0 none;
  padding: 0 8px;
  background: none;
  text-align: center;
  overflow: hidden;
  color: $colorTextBody;
  @include user-select(none);

}

.js-qty__adjust--plus {
  right: 0;
}

.js-qty__adjust--minus {
  left: 0;
}

/*============================================================================
  #Quantity Selectors in Ajax Cart
==============================================================================*/
.ajaxcart__qty {
  @extend .js-qty;
  margin: 0;

  .is-loading & {
    opacity: 0.5;
    transition: none;
  }
}

.ajaxcart__qty-num {
  color: colorTextBody !important;
}

.ajaxcart__qty-adjust {
  @extend .js-qty__adjust;
}

.ajaxcart__qty--plus {
  @extend .js-qty__adjust--plus;
}

.ajaxcart__qty--minus {
  @extend .js-qty__adjust--minus;
}

.site-nav {
  display: inline-block;
}

#CartContainer {
  .price {margin-bottom:0px;}
}

#CartDrawer {
  z-index: 900;
}


/*================ Hero ================*/

.hero-section{
  position: relative;
}

.secondary-call-to-action {
   @include size-body(14px);
  letter-spacing: 2px;
  position: relative;
  text-transform: uppercase;
  color: $colorTextHeading;
  font-family:$buttonfont;

  &:hover,
  &:focus,
  &:active {
      transition: all 0.2s ease;
      color: darken($colorTextHeading,10%);
  }
}
.btn.call-to-action.hero-btn {
    border: 2px solid;
    transition: all .3s;
    -webkit-transition: all .3s;
    -moz-transition: all .3s;
    -ms-transition: all .3s;
  transition: all .3s;
}

.no-js {
  .video {opacity:1;}
}

.video {
  overflow:hidden;
  position: relative;
  background-repeat: no-repeat;
  background-size: cover;
}

.centered-content {
  text-align: center;
  margin-left: auto;
  margin-right: auto;
  display: block;
}

.filter{
  position: absolute;
  background-color:rgba(0,0,0,.4);
  width: 100%;
  height: 100%;
  top:0px;
  z-index: 1;
  display: none;
}

.call-to-action {margin-right:15px;}

.call-to-action-circle{
  position: relative;
  font-size: 30px;
  vertical-align: middle;
  bottom: -3px;
}

.call-to-action-triangle{
  @include size-body(14px);
  margin: auto;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 10px;
  height: 15px;
  margin-top: -11px;
  margin-left: -12px;
}
/*================ Featured Page ================*/

.featured-page{
  display:flex;
  flex-flow:row wrap;
  align-items:center;
  margin:3px 0;
  &.text-is-left{flex-flow:row-reverse wrap;}
  .featured-heading{
    display:none;
    @include at-query($min,$large){
      display:flex;
      position:relative;
      background:#C4C4C4;
      color:#fff;
      flex-basis:40%;
      //max-width:420px;
      text-align:center;
      flex-flow:column nowrap;
      justify-content:space-around;
      height:535px;
      
      &:after{
        content:'';
        width: 0;
        height: 0;
        border-style: solid;
        border-width: 30px 0 30px 24px;
        border-color: transparent transparent transparent #c4c4c4;
        position:absolute;
        top:calc(50% - 15px);
        right:-24px;
        display:block;
      }
      
      .num{
        font-weight:bold;
        text-align:center;
        line-height:43px;
        border:1px solid #fff;
        border-radius:100%;
        height:43px;
        width:43px;
        margin:0 auto;
      }
      .large-title{
        font-size:45px;
        font-weight:bold;
      }
      .small-title{
        font-size:30px; 
      }
    }
  }
  &.text-is-left .featured-heading:after{right:auto; left:-24px;  border-width: 30px 24px 30px 0; border-color: transparent #c4c4c4 transparent transparent;}
  .featured-text{
    padding:$gutter/2 $gutter;
    @include size-body(18px);
    text-align:center;
    @include at-query($min,$large){
      flex-basis:60%;
      padding:$gutter/1 $gutter*2.5;
    }
    .text-wrap{ 
      text-align:left;
      max-width:410px;
      margin:0 auto;
      @include at-query($max,$medium){
        margin:0 auto;
      }
    }
    a{font-weight:bold; color:$colorAccentAlt;}
    .text-center span{font-size:36px;}
   
  }
}


