  z-index: 1000;
}


/*================ Use em() Sass function to declare font-size ================*/
h1 {
  @include size-header(36px);
  
}

h2 {
  @include size-header(28px);
  
}

h3 {
  @include size-header(22px);
 
}

h4 {
  @include size-header(20px);
  
}

h5 {
  @include size-header(16px);
  
}

h6 {
  @include size-header(14px);
 
}


.h1 { @extend h1; }
.h2 { @extend h2; }
.h3 { @extend h3; }
.h4 { @extend h4; }
.h5 { @extend h5; }
.h6 { @extend h6; }

p {

  img {
    margin: 0;
  }
}

em {
  font-style: italic;
}

b, strong {
  font-weight: bold;
}

small {
  @include size-body(12px);
}

sup, sub {
  position: relative;
  font-size: 60%;
  vertical-align: baseline;
}
sup {
  top: -0.5em;
}

sub {
  bottom: -0.5em;
}

.light-gray {color:$colorTextLight;}

/*================ Blockquotes ================*/
blockquote {
  @include size-body(15px,5);
  line-height: 1.45;
  font-style: italic;
  margin: 0 0 $gutter;
  padding: ($gutter / 2) $gutter;

  p {
    margin-bottom: 0;

    & + cite {
      margin-top: $gutter / 2;
    }
  }

  cite {
    display: block;
    @include size-body(11px);

    &:before {
      content: '\2014 \0020';
    }
  }
}

/*================ Code ================*/
code, pre {
  background-color: #faf7f5;
  font-family: Consolas,monospace;
  @include size-body(14px);
  border: 0 none;
  padding: 0 2px;
  color: #51ab62;
}

pre {
  overflow: auto;
  padding: $gutter / 2;
  margin: 0 0 $gutter;
}

/*================ Horizontal Rules ================*/
hr {
  clear: both;
  border-top: solid #E5E5E5;
  border-width: 1px 0 0;
  margin: $gutter auto;
  height: 0;

  &.hr--small {
    margin: ($gutter / 2) auto;
  }
  &.hr--med{
     margin:$gutter/2 auto;
    max-width:150px;
  }

  &.hr--clear {
    border-top-color: transparent;
  }
}

/*================ Section Headers ================*/

.main-heading {
  @include size-header(48px);
  margin-bottom:0px;
  color: $colorTextHeading;
}
.main-subheading {
  color: $colorTextBody;
  display: block;
  font-family: $body_font;
}
.two-subheading { font-size: 14px*3.5 ; }
.one-five-subheading { font-size: 14px*2.5 ; }
.one-three-subheading { font-size: 14px*1.3; }
.one-subheading {@include size-header(14px); }
.point-nine-subheading { font-size: 14px*.9; }

.section-header {
  padding-top: $gutter;
  padding-bottom: $gutter;
  text-align: center;
}

.notransition * { transition: none !important; } 
.site-header {
  background: {{ settings.color_header_bg }} none repeat scroll 0 0;
  position: absolute;
  width: 100%;
  z-index: 800 !important;
  -webkit-overflow-scrolling: touch;
  .template-list-collections &{
    background: $collectionHeaderBackground none repeat scroll 0 0;
  }
  .template-collection &{
    background: $collectionHeaderBackground none repeat scroll 0 0;
  }
  .grid--table {padding: 10px 0 10px;}
  .logo{-webkit-filter: invert(1); filter: invert(1);}
  .template-index & .logo,
  .template-product  & .logo,
  .template-page:not(.page-contact) & .logo,
  .template-cart & .logo, 
  .template-search & .logo{ -webkit-filter:invert(0); filter: invert(0);}
  .page-contact &,
  .template-customers-login &,
  .template-blog &{background:#fff;}
}

.section-header div {
  padding: 0 6px;
}
@include at-query ($min, $large) {
  .section-header {
    display: table;
    width: 100%;
  }

  .section-header__title {
    margin-bottom: $gutter / 2.5;
  }

  .section-header__left {
    display: table-cell;
    margin-bottom: 0;

    h1, h2, h3, h4,
    .h1, .h2, .h3, .h4 {
      margin-bottom: 0;
    }
  }

  .section-header__right {
    display: table-cell;
    vertical-align: middle;
    text-align: right;
    width: 335px;

    @include at-query ($max, $medium) {
      margin-bottom: $gutter;
    }
  }
}

.section-header__right {
  .form-horizontal,
  .collection-view {
    display: inline-block;
    vertical-align: middle;
  }

  @include at-query ($min, $postSmall) {
    label + select,
    .collection-view {
      margin-left: $gutter / 2;
    }
  }
}


/*============================================================================
  #Rich Text Editor
==============================================================================*/
.rte {
  margin-bottom: $gutter / 2;
  @include size-button(14px,14);
  a {
    text-decoration: underline;
  }

  // Add some top margin to headers from the rich text editor
  h1, h2, h3, h4, h5, h6 {
    margin-top: 2em;

    &:first-child {
      margin-top: 0;
    }

    a {
      text-decoration: none;
    }
  }

  > div {
    margin-bottom: $gutter / 2;
  }

  li {
    margin-bottom: 0.4em;
  }
}

.rte--header {
  margin-bottom: 0;
}

/*============================================================================
  #Links and Buttons
==============================================================================*/
a,
.text-link {
  color: $colorLink;
  text-decoration: none;
  background: transparent;
}

h1,h2,h3,h4,h5,h6 {
  a {
    color:$colorTextHeading !important;
  }
}

a:hover,
a:focus {
  color: $colorLinkHover;
}

button {
  overflow: visible;
}

button[disabled],
html input[disabled] {
  cursor: default;
}

.btn,
.rte .btn {
  background: #fff;
  color: $colorAccentAlt;
  @include size-button(12px);
  padding: 15px 30px;
  text-transform: uppercase;
  display: inline-block;
  letter-spacing: 2px;
  border: 1px solid $colorAccentAlt;
  font-family: $buttonfont;
  font-weight:bold;
  transition: all 0.2s ease;
  line-height: 1.5;

  /*================ Set primary button colors ================*/
  &:hover,
  &:active,
  &:focus {
      background: lighten($colorBtnPrimary,5%);
      border: 1px solid lighten($colorBtnBorder,5%);
      color: lighten($colorBtnText,10%);
  }
  &.teal{
    background:$colorBtnPrimary;
    color:$colorBtnText;
  }
  &.slider-btn{
    background:none;
    border: none;
  }    
  &.white-border{
    border:1px solid #fff;
    background:none;
    color:$colorBtnText;
  }
  &.orange{
    border: 1px solid #eeb237;
    background: #eeb237;;
    color: #fff;
    &:hover {
      background: #fff;
      color: lighten($colorBtnPrimary,5%);
      border: 1px solid #00b0b9;
    }
  }    
  &.large{display:block; padding:10px 35px; width:100%; max-width:375px; margin-left:auto; margin-right:auto;}
}

.no-buttons .btn {display:none;}

button.btn {
  display: block;
  margin-left: auto;
  margin-right: auto;
}

.btn--secondary,
.rte .btn--secondary {
  @extend .btn;
}

.btn--small {
  padding: 7px 10px;
  @include size-button(14px);
  background: $colorBtnPrimary;
  color: $colorBtnText;
  text-transform: uppercase;
  display: inline-block;
  letter-spacing: 2px;
  border: 2px solid $colorBtnBorder;
  font-family: $buttonfont;
  transition: all 0.2s ease;
  line-height: 1.5;

  /*================ Set primary button colors ================*/
  &:hover,
  &:active,
  &:focus {
      background: darken($colorBtnPrimary,10%);
      border: 2px solid darken($colorBtnBorder,10%);
      color: darken($colorBtnText,10%);
  }

}


.btn--full {
  width: 100%;
}

/*================ Force an input/button to look like a text link ================*/
.text-link {
  display: inline;
  border: 0 none;
  background: none;
  padding: 0;
  margin: 0;
}

/*============================================================================
  #lists
==============================================================================*/
ul, ol {
  margin: 0 0 ($gutter / 2) 20px;
  padding: 0;
}

ol { list-style: decimal; }
ul ul, ul ol,
ol ol, ol ul { margin: 4px 0 5px 20px; }
li { margin-bottom: 0.25em; }

ul.square { list-style: square outside; }
ul.disc { list-style: disc outside; }
ol.alpha { list-style: lower-alpha outside; }

.no-bullets {
  list-style: none outside;
  margin-left: 0;
}

.inline-list {
  margin-left: 0;

  li {
    display: inline-block;
    margin-bottom: 0;
  }
}

/*============================================================================
  #Tables
==============================================================================*/
table {
  width: 100%;
  border-collapse: collapse;
  border-spacing: 0;
}

table.full {
  width: 100%;
  margin-bottom: 1em;
}

.table-wrap {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

th, td {
  text-align: left;
  padding: 20px;
  font-weight: inherit !important;
}

/*============================================================================
  Responsive tables, defined with .table--responsive on table element.
  Only defined for IE9+
==============================================================================*/
.table--responsive {
  @include at-query(null, 0px, 991px) {
    .cart__row {padding-top:0px;}
    th {padding: 10px 10px 0px 10px;}
    .cart__row:first-child {border:none;}
    h2, h3 {text-align:center;}
    .cart__row:first-child {margin-top:0px;}
    .cart__subtotal {margin-left:0px !important;}

    thead {
      display: none;
    }

    tr {
      display: block;
    }

    // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      text-align: right;
      padding: 15px;
    }

    td:before {
      content: attr(data-label);
      float: left;
      font-size: 12px;
      position: absolute;
      left: 0;
    }

    .product-name-label {color:transparent;}

    &.cart-table {
      img {
        margin: 0 auto;
      }
    }
  }
}

@include at-query($max, $small) {
  .table--small-hide {
    display: none !important;
  }

  .table__section + .table__section {
    position: relative;
    margin-top: 10px;
    padding-top: 15px;

    &:after {
      content: '';
      display: block;
      position: absolute;
      top: 0;
      left: 15px;
      right: 15px;
    }
  }
}


/*============================================================================
  #OOCSS Media Object
    - http://www.stubbornella.org/content/2010/06/25/the-media-object-saves-hundreds-of-lines-of-code/
==============================================================================*/
.media,
.media-flex {
  overflow: hidden;
  _overflow: visible;
  zoom: 1;
}

.media-img {
